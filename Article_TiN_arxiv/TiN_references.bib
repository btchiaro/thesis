	Issn = {0167-2738},
	Journal = {Solid State Ionics},
	Keywords = {Titanium oxynitrides, Thin films, Impedance spectroscopy, Rutherford backscattering},
	Note = {Proceedings of the 17th International Conference on Solid State Ionics},
	Number = {1},
	Pages = {693 - 698},
	Title = {Chemical composition, crystallographic structure and impedance spectroscopy of titanium oxynitride TiNxOy thin films},
	Url = {http://www.sciencedirect.com/science/article/pii/S0167273810004194},
	Volume = {192},
	Year = {2011},
